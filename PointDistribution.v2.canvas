        {
          "objectType" : "ExecPort",
          "name" : "handle",
          "nodePortType" : "Out",
          "execPortType" : "In",
          "typeSpec" : "FbxHandle"
          },
        {
          "objectType" : "ExecPort",
          "name" : "path",
          "nodePortType" : "Out",
          "execPortType" : "In",
          "typeSpec" : "String"
          },
        {
          "objectType" : "ExecPort",
          "name" : "mesh",
          "nodePortType" : "In",
          "execPortType" : "Out",
          "typeSpec" : "PolygonMesh"
          }
        ],
      "extDeps" : {
        "Fbx" : "*"
        },
      "presetGUID" : "29476BA029342546549C4F9AAC3294E8",
      "code" : "dfgEntry {
  mesh = PolygonMesh();
  
  ImporterIdentifier identifier;
  identifier.path = path;
  identifier.type = \"MESH\";
  
  UInt32 polygonSizes[];
  UInt32 packedPolyPointIndices[];
  Vec3 positionsData[];
  Vec3 polyPointNormalsData[];
  Vec2 polyPointUVsData[][];
  Color polyPointColors[][];

  FbxHandle handleCopy = handle;
  handleCopy.parsePolygonMeshAndAttributes( identifier, polygonSizes, packedPolyPointIndices, positionsData, polyPointNormalsData, polyPointUVsData, polyPointColors);
  UInt32 nbPoints = positionsData.size();
  UInt32 nbPolys = polygonSizes.size();

  mesh.clear();

  mesh.createPoints( nbPoints );
  mesh.addPolygons( polygonSizes, packedPolyPointIndices );

  Ref<Vec3Attribute> normals = mesh.getOrCreateNormals();

  for(Size i = 0; i < nbPoints; ++i )
    mesh.setPointPosition( i, positionsData[i] );
  mesh.incrementPointPositionsVersion();

  mesh.setAttributeFromPolygonPackedData( 'normals', polyPointNormalsData );
  normals.incrementVersion();
  if( polyPointUVsData.size() != 0 ) {
    for(Size i = 0; i < polyPointUVsData.size(); ++i )
      mesh.setAttributeFromPolygonPackedData( 'uvs'+i, polyPointUVsData[i] );
  }
  if( polyPointColors.size() != 0 ) {
    for(Size i = 0; i < polyPointColors.size(); ++i ) {
      String name = 'vertexColors';
      if(i > 0)
        name += i;
      mesh.setAttributeFromPolygonPackedData( name, polyPointColors[i] );
    }
  }
}
"
